<h1>CELEBRITIES</h1>
<a href="/celebrity/add">ADD A NEW CELEBRITY!</a>
{{#each celebrities}}
<p><a href="celebrity/{{this._id}}">{{this.name}}</a>

<a href="/celebrity/edit?celebrity_id={{this._id}}" class="edit-button">EDIT</a>
<a href="/celebrity/delete?celebrity_id={{this._id}}" class="edit-button" style="background-color: red">DELETE</a></p>


{{/each}}